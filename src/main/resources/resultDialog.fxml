<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Label?>
<BorderPane xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.uploaderclient.gui.result_dialog.control.DialogController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <top>
        <Text fx:id="titleLabel" text="Upload started">
            <font>
                <Font size="20.0" />
            </font>
        </Text>
    </top>
    <center>
        <Label fx:id="valuesLabel"/>
    </center>
    <bottom>
        <AnchorPane>
            <Button fx:id="closeButton" onAction="#closeWindow" text="Ok" visible="false" AnchorPane.bottomAnchor="0" AnchorPane.rightAnchor="0"/>
        </AnchorPane>
    </bottom>
</BorderPane>
